unit Types;

interface

type TFigures = array[1..8,1..8] of integer;
     {Figurky :
      nic         0
      pesiak      1
      veza        2
      kon         3
      strelec     4
      kralovna    5
      kral        6}

     TSur = record
       X, Y : integer;
     end;

     TMove = record
       A,B : TSur;
     end;


implementation

end.
